<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: Monument completed -->
  <BuildMonumentWorker.LetterLabelMonumentConstructed.slateRef>Монумент завершён</BuildMonumentWorker.LetterLabelMonumentConstructed.slateRef>
