    <li>founder_died(founder_gender==Male)->умер</li>
    <li>founder_died(founder_gender==Female)->умерла</li>
    <li>guilty_founder_showed(founder_gender==Male)->показал</li>
    <li>guilty_founder_showed(founder_gender==Female)->показала</li>
    <li>guilty_founder_demanded(founder_gender==Male)->потребовал</li>
    <li>guilty_founder_demanded(founder_gender==Female)->потребовала</li>
    <li>guilty_founder_learned(founder_gender==Male)->понял</li>
    <li>guilty_founder_learned(founder_gender==Female)->поняла</li>
  </Guilty.descriptionMaker.rules.rulesStrings>
